@model IEnumerable<WebStore.ViewModels.EmployeeViewModel>

<table class="table">
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.FirstName</td>
        <td>@item.SurName</td>
        <td>@item.Patronymic</td>
        <td>@item.Age</td>
        <td>
            @Html.ActionLink("Детали", "EmployeeDetails", new { id = item.id })
        </td>
    </tr>
    }
</table>
