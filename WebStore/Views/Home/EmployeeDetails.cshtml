@model WebStore.ViewModels.EmployeeViewModel

<table class="table table-bordered">
    <tr>
        <td>Id</td>
        <td>@Model.id</td>
    </tr>
    <tr>
        <td>Фамилия</td>
        <td>@Model.SurName</td>
    </tr>
    <tr>
        <td>Имя</td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Отчество</td>
        <td>@Model.Patronymic</td>
    </tr>
    <tr>
        <td>Возраст</td>
        <td>@Model.Age</td>
    </tr>
    <tr>
        <td>Должность</td>
        <td>@Model.Position</td>
    </tr>
    <tr>
        @Html.ActionLink("Назад", "Employees")
    </tr>
</table>